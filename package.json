{
  "name": "worldofchatting",
  "version": "1.0.0",
  "main": "",
  "author": "Steven Levine (https://github.com/slevine111)",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "webpack": "webpack",
    "initialize": "mkdir js && npm run seed",
    "compile-and-start": "tsc && webpack && node ./js/server",
    "build": "tsc",
    "tsc:watch": "tsc -w",
    "seed": "node ./js/bin/seed_all.js",
    "tsc:clean-and-build": "rm -rf js && tsc",
    "tsc:build:ifnotbuilt": "[ -d ./js/bin ] || tsc",
    "client:build": "tsc && webpack",
    "client:dev:tsc": "NODE_ENV=development [ -d ./js/client ] || tsc && (tsc -w & webpack -w)",
    "server:dev:ts": "NODE_ENV=development [ -d ./js/server ] || tsc && (tsc -w & nodemon ./js/server --watch ./js/server -e js)",
    "server:dev": "LOAD_CONFIG_FILE=true nodemon ./js/server --watch ./js/server -e js",
    "client:dev": "NODE_ENV=development webpack -w",
    "start:local:dev": "npm run tsc:build:ifnotbuilt && (tsc -w & npm run client:dev & npm run server:dev)",
    "start:local": "tsc && NODE_ENV=development LOAD_CONFIG_FILE=true node ./js/server",
    "start:dev": "NODE_ENV=development webpack -w & NODE_ENV=development nodemon ./js/server --watch ./js/server -e js",
    "test": "jest --runInBand --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.9.10",
    "@nestjs/common": "^6.5.0",
    "@nestjs/core": "^6.5.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/microservices": "^6.11.8",
    "@nestjs/platform-express": "^6.5.2",
    "@nestjs/platform-socket.io": "^6.11.8",
    "@nestjs/serve-static": "^1.0.1",
    "@nestjs/typeorm": "^6.1.3",
    "@nestjs/websockets": "^6.11.8",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "cache-manager": "^2.10.0",
    "cache-manager-redis-store": "^1.5.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "faker": "^4.1.0",
    "moment": "^2.24.0",
    "node-notifier": "5.4.3",
    "pg": "^7.11.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-wordcloud": "^1.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "socket.io-client": "^2.3.0",
    "typeorm": "^0.2.18",
    "typescript": "^3.6.3",
    "volleyball": "^1.5.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.5.2",
    "@types/bcrypt": "^3.0.0",
    "@types/cache-manager": "^2.10.0",
    "@types/cheerio": "^0.22.13",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/faker": "^4.1.9",
    "@types/jest": "^24.0.15",
    "@types/material-ui": "^0.21.7",
    "@types/node": "^12.0.12",
    "@types/pg": "^7.4.14",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.5",
    "@types/redis": "^2.8.16",
    "@types/redux-logger": "^3.0.7",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "dts-gen": "^0.5.8",
    "jest": "^24.8.0",
    "jest-junit": "^9.0.0",
    "nodemon": "^1.19.1",
    "redux-logger": "^3.0.6",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8"
  }
}
