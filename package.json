{
  "name": "worldofchatting",
  "version": "1.0.0",
  "main": "",
  "author": "Steven Levine (https://github.com/slevine111)",
  "license": "MIT",
  "scripts": {
    "initialize": "mkdir js && yarn run build && yarn run seed",
    "compile-and-start": "tsc && webpack && node ./js/server",
    "seed": "node ./js/bin/seed.js",
    "ts:clean-and-build": "rm -rf js && tsc",
    "ts:build:ifnotbuilt": "[ -d ./js/bin ] || tsc",
    "client:build": "tsc && webpack",
    "client:dev": "[ -d ./js/client ] || tsc && (tsc -w & webpack -w)",
    "server": "tsc && node ./js/server",
    "server:dev": "[ -d ./js/server ] || tsc && (tsc -w & nodemon ./js/server --watch ./js/server -e js)",
    "start:dev": "yarn run ts:build:ifnotbuilt && (tsc -w & webpack -w & nodemon ./js/server --watch ./js/server -e js)",
    "test": "jest ./tests/**/*.ts"
  },
  "dependencies": {
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.4.1",
    "@material-ui/styles": "^4.4.1",
    "@nestjs/common": "^6.5.0",
    "@nestjs/core": "^6.5.0",
    "@nestjs/platform-express": "^6.5.2",
    "@nestjs/serve-static": "^1.0.1",
    "@nestjs/typeorm": "^6.1.3",
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.0.0",
    "pg": "^7.11.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "typeorm": "^0.2.18",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.5.2",
    "@types/cheerio": "^0.22.13",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.15",
    "@types/material-ui": "^0.21.7",
    "@types/node": "^12.0.12",
    "@types/pg": "^7.4.14",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.5",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8"
  }
}
